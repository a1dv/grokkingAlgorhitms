#–ü—Ä–∏–º–µ—Ä –≥–æ–ª–æ—Å–æ–≤–∞–Ω–∏—è

>>> voted = {}
>>> value = voted.get(‚Äútom‚Äù)

#–ö–æ–¥

voted = {}
def check_voter(name):
	if voted.get(name):
		print "kick them out!"
	else:
		voted[name] = True
 		print "let them vote!"

check_voter("tom")   #let them vote!
check_voter("mike")  #let them vote!
check_voter("mike")  #kick them out!

#–ö–æ–¥ —Ä–∞–±–æ—Ç–∞–µ—Ç –∫–æ—Ä—Ä–µ–∫—Ç–Ω–æ


#–ö—ç—à
cache = {}

def get_page(url):
	if cache.get(url):
		return cache[url] 
	else:
 		data = get_data_from_server(url)
		cache[url] = data 
 		return data



